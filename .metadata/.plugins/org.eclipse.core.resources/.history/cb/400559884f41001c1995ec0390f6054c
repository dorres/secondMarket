package first.market.kurlyty.user.dao;



import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;


import first.market.kurlyty.user.vo.user_address_listVO;

@Repository
public class user_address_listDAO {
	@Autowired
	private SqlSessionTemplate sqlSession;
	
public user_address_listVO getBoard(user_address_listVO vo) throws Exception {
		
		return sqlSession.selectOne("user_address_listDAO.getAddress", vo);
	}
	
	

}
