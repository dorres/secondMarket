package first.market.kurlyty.user.dao;



import javax.servlet.http.HttpSession;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import first.market.kurlyty.user.vo.user_address_listVO;

@Repository
public class user_address_listDAO {
	@Autowired
	private SqlSessionTemplate sqlSession;
	
public user_address_listVO getBoard(user_address_listVO vo) throws Exception {
		HttpSession session = null;
		session.getAttribute("userId");
		session.setAttribute("userId", "user_id");
		
		return (user_address_listVO)sqlSession.selectOne("user_address_listDAO.getAddress", vo);
	}
	
	

}
