<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="pointDAO">
 <select id="pointList" resultType="orderListVO">
  select * from mk_order where user_id=#{user_id}
 </select>
 <select id="totalPoint" resultType="java.lang.Integer">
 select sum(order_point) from mk_order where user_id=#{user_id}
 </select>
 </mapper>