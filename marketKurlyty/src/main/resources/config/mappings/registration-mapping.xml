<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="RegistDAO">
	<insert id="insertRegist" parameterType="regist">
		INSERT INTO MK_GOODS_DETAIL VALUES(#{goods_detail_serial},#{categoty_main_serial},#{categoty_sub_serial},#{categoty_goods_serial},
		#{goods_detail_price},#{goods_detail_stock_quantity},#{goods_detail_stock_notification},#{goods_detail_promotion_serial},#{goods_detail_status},#{goods_detail_dicountrate});
	</insert> 
	
	<select id="getGoods" resultType="regist">
		SELECT * FROM MK_GOODS_DETAIL;
	</select>
	
 </mapper>